{
  "name": "select-sires-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "tsc ./src/set-env.ts && node ./src/set-env.js",
    "setBundleId": "node ./src/set-bundle-id.js",
    "start": "ng serve --port 4242 -o",
    "build": "npm run config && ionic build --prod  && npx cap update && npx cap copy && npx cap sync",
    "buildWeb": "npm run config  && ng build --prod --aot",
    "mobile": "npm run config && npx cap update && ionic build --prod && npx cap copy && npx cap sync",
    "android": "npx cap open android",
    "ios": "npx cap open ios",
    "buildDeploy": "npm run config  &&  ng build --prod --aot && firebase deploy --project select-sires --only hosting",
    "test": "npm run config && ng test --no-watch --source-map=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "npm run config  &&  ng e2e",
    "analyze": "npm run config  && ng build --prod --stats-json && webpack-bundle-analyzer ./dist/select-sires-app/stat-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/link-preview": "^1.1.1",
    "@angular/animations": "9.0.5",
    "@angular/cdk": "9.1.3",
    "@angular/common": "9.0.5",
    "@angular/compiler": "9.0.5",
    "@angular/core": "9.0.5",
    "@angular/fire": "6.0.0",
    "@angular/forms": "9.0.5",
    "@angular/material": "9.1.3",
    "@angular/platform-browser": "9.0.5",
    "@angular/platform-browser-dynamic": "9.0.5",
    "@angular/router": "9.0.5",
    "@capacitor/android": "2.0.1",
    "@capacitor/core": "2.0.1",
    "@capacitor/ios": "2.0.1",
    "@ionic-native/core": "5.23.0",
    "@ionic-native/splash-screen": "5.23.0",
    "@ionic/angular": "5.0.5",
    "@ngrx/effects": "8.6.0",
    "@ngrx/entity": "8.6.0",
    "@ngrx/store": "8.6.0",
    "@ngrx/store-devtools": "8.6.0",
    "angular-bootstrap-md": "9.0.0",
    "animate.css": "3.7.2",
    "br-mask": "0.0.9",
    "chart.js": "2.5.0",
    "core-js": "2.5.4",
